# IMPORTANT: colors have to be defined in quotation marks.
# Otherwise the "#" and following would be interpreted as a comment.
[urgency_low]
    background = "{background}80"
    foreground = "{color2}"
	frame_color = "{color10}"
    highlight = "{color6}"

[urgency_normal]
    background = "{background}"
    foreground = "{foreground}"
	frame_color = "{color8}"
    highlight = "{color6}"

[urgency_critical]
    background = "{color1}"
    foreground = "{foreground}"
    frame_color = "{color0}"
    highlight = "{color6}"
    # icon = 

